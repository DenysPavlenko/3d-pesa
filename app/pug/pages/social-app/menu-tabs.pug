include /partials/menu-tabs
-
  var tabs = [
    {link:'social-app-feed.html', title:'Feed', isActive:tabName === 'feed', icon:{name:'house', width:'21', height:'20'}},
    {link:'social-app-profile.html', title:'Profile', isActive:tabName === 'profile', icon:{name:'profile', width:'18', height:'20'}},
    {link:'social-app-chat.html', title:'Chat', isActive:tabName === 'chat', icon:{name:'chat', width:'20', height:'20'}, badge: {cnt: 3, color: 'primary', round: true}},
    {link:'social-app-contacts.html', title:'Contacts', isActive:tabName === 'contacts', icon:{name:'contacts', width:'20', height:'20'}},
  ]

+menu-tabs(tabs)
